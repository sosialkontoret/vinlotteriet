{
  "extends": "tslint-config-airbnb",
  "rules": {
    "max-line-length": [
      true,
      {
        "limit": 140,
        "ignore-pattern": "^import [^,]+ from |^export | implements "
      }
    ],
    "directive-selector": [
      true,
      "attribute",
      "sk",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "sk",
      "kebab-case"
    ]
  },
  "rulesDirectory": [
    "codelyzer"
  ]
}
